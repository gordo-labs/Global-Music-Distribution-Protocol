  "scripts": {
    "test": "npx hardhat test",
    "serve": "npx hardhat node",
    "deploy:local": "npx hardhat run scripts/deploy.ts --network localhost",
    "deploy:mumbai": "npx hardhat run scripts/deploy.ts --network mumbai",
    "deploy:rinkeby": "npx hardhat run scripts/deploy.ts --network rinkeby",
    "deploy:sepolia": "npx hardhat run scripts/deploy.ts --network sepolia",
    "deploy:ropsten": "npx hardhat run scripts/deploy.ts --network ropsten",
    "compile": "npx hardhat compile",
    "size": "yarn run hardhat size-contracts",
    "console": "npx hardhat console --network localhost",
    "remix": "remixd -s ./ --remix-ide https://remix.ethereum.org"
  },
  "repository": {
    "type": "git",
